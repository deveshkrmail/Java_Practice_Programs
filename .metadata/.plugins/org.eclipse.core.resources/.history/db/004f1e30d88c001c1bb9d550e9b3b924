package b;

public class B3_Data_types_in_Java_2_char 
{

	public static void main(String[] args) 
	{
		char cha01='I';
		char cha02='N';
		System.out.println("cha01="+cha01);
		System.out.println("cha02="+cha02);
		
		char cha03='D';
		char cha04='I';
		System.out.println("cha03="+cha03);
		System.out.println("cha04="+cha04);

	}

}
