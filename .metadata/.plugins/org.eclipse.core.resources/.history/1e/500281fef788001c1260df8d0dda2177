package nineth;
import java.util.*;
public class P147Q31 {

	public static void main(String[] args) 
	{
		int n02;
		Scanner sc=new Scanner(System.in);
		System.out.println("Please enter an integer:	");
		int n01=sc.nextInt();
		while(n01>0)
		{
			int d=n01%10;
			if(d%2!=0)
			{
				n02
			}	
			n01=n01/10;
		}
		
	}

}
